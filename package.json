{
  "name": "@stencil/screenshot",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Screenshot Visual Diff App",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --es5 && npm run remove.svgs",
    "remove.svgs": "rimraf ../stencil/screenshot/app/build/app/svg",
    "start": "stencil build --dev --es5 --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watch"
  },
  "dependencies": {
    "@ionic/core": "4.0.0-beta.7",
    "@stencil/core": "0.13.0-3",
    "pixelmatch": "^4.0.2"
  },
  "devDependencies": {
    "@stencil/router": "^0.3.0",
    "@types/jest": "23.3.1",
    "@types/pixelmatch": "^4.0.0",
    "jest": "23.5.0",
    "jest-environment-node": "23.4.0",
    "puppeteer": "1.7.0",
    "rimraf": "2.6.2",
    "workbox-build": "^3.4.1"
  },
  "author": "Ionic Team",
  "license": "MIT"
}
